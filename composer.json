{
    "name": "lemberg/laravel-query-logs",
    "description": "",
    "license": "Apache-2.0",
    "homepage": "https://github.com/lemberg/laravel-query-logs",
    "keywords": ["laravel", "query", "log"],
    "authors": [
        {
            "name": "Nazar Saikevych",
            "email": "nazar.saikevych@lemberg.co.ua"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.*|5.5.*|5.6.*",
        "laravel/tinker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Lemberg\\LaravelQueryLogs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemberg\\LaravelQueryLogs\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}